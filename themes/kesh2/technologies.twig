{% include 'incl/header.twig' %}

<body>
    {% include 'incl/nav.twig' %}

    <div class="main" role="main">
        <div class="landing-page">
            {{ content }}

            {% for tech in meta.Techs %}
                <a href="{{ base_url }}/technology/{{ tech.class }}" class="box {{ tech.class }}">
                    <h2>{{ tech.name }}</h2>
                    <i class="fa-brands fa-{{ tech.icon }}" aria-hidden="true"></i>
                </a>
            {% endfor %}
        </div>
    </div>
    
    {% include 'incl/footer.twig' %}

</body>

</html>
